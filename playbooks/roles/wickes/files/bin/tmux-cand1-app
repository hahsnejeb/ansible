#!/bin/bash

SESSION=tmux-cand1-app

tmux new-session -d -s $SESSION

tmux split-window -h
tmux select-pane -t 0
tmux send-keys "ssh -i ~/.ssh/wickestest03.pem ubuntu@10.34.120.36" C-m
tmux select-pane -t 1
tmux send-keys "ssh -i ~/.ssh/wickestest03.pem ubuntu@10.34.121.37" C-m
tmux split-window -v
tmux send-keys "ssh -i ~/.ssh/wickestest03.pem ubuntu@10.34.122.28" C-m

# Attach to session
tmux attach-session -t $SESSION
