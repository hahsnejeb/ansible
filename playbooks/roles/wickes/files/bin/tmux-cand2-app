#!/bin/bash

SESSION=tmux-cand2-app

tmux new-session -d -s $SESSION

tmux split-window -h
tmux select-pane -t 0
tmux send-keys "ssh -i ~/.ssh/wickestest03.pem ubuntu@10.34.120.68" C-m
tmux select-pane -t 1
tmux send-keys "ssh -i ~/.ssh/wickestest03.pem ubuntu@10.34.121.85" C-m
tmux split-window -v
tmux send-keys "ssh -i ~/.ssh/wickestest03.pem ubuntu@10.34.122.222" C-m

# Attach to session
tmux attach-session -t $SESSION
